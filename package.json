{
  "name": "v-digital-time-picker",
  "version": "0.1.8",
  "description": "Digital Time Picker for Vuetify",
  "author": "Runyasak Chaengnaimuang",
  "license": "MIT",
  "homepage": "https://github.com/runyasak/v-digital-time-picker",
  "repository": {
    "type": "git",
    "url": "https://github.com/runyasak/v-digital-time-picker.git"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "src/*",
    "types/*"
  ],
  "main": "./dist/v-digital-time-picker.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:bundle": "rm -rf dist && vue-cli-service build --target lib src/entry.ts --mode production && copyfiles --flat helper-json/*.json dist/vetur",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-watch": "vue-cli-service test:unit --watch",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "typescript",
    "vuetify",
    "vue"
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "copyfiles": "^2.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "~3.8.3",
    "vee-validate": "^3.3.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  }
